<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="ru.ifmo.java.gui.Controller">
    <children>
        <GridPane alignment="CENTER" cacheShape="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <StackPane>
                    <children>
                        <VBox alignment="CENTER">
                            <children>
                                <GridPane alignment="CENTER">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    </rowConstraints>
                                    <children>
                                        <Label text="TypeOfVariableParameter: "/>
                                        <ChoiceBox fx:id="typeOfVariableParameterChoiceBox" prefWidth="150.0"
                                                   GridPane.columnIndex="1"/>
                                        <Label text="TypeOfComputeServer: " GridPane.rowIndex="1"/>
                                        <ChoiceBox fx:id="typeOfServerChoiceBox" prefWidth="150.0"
                                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                        <Label text="StartOfVariableParameter: " GridPane.rowIndex="2"/>
                                        <TextField fx:id="startOfVariableParameter" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="2"/>
                                        <Label text="EndOFVariableParameter: " GridPane.rowIndex="3"/>
                                        <TextField fx:id="endOfVariableParameter" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="3"/>
                                        <Label text="DeltaOfVariableParameter: " GridPane.rowIndex="4"/>
                                        <TextField fx:id="deltaOfVariableParameter" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="4"/>
                                        <Label text="NumberOfClients:" GridPane.rowIndex="5"/>
                                        <Label text="NumberOfRequestsByClient:" GridPane.rowIndex="6"/>
                                        <Label text="SleepTime:" GridPane.rowIndex="8"/>
                                        <TextField fx:id="numberOfClientsTextField" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="5"/>
                                        <TextField fx:id="numberOfRequestsByClient" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="6"/>
                                        <TextField fx:id="sleepTimeTextField" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="8"/>
                                        <Label text="RequestSize: " GridPane.rowIndex="7"/>
                                        <TextField fx:id="requestSizeTextField" GridPane.columnIndex="1"
                                                   GridPane.rowIndex="7"/>
                                    </children>
                                </GridPane>
                                <Button fx:id="runningButton" onAction="#run" text="Run"/>
                                <HBox alignment="CENTER">
                                    <children>
                                        <Label text="Status: "/>
                                        <Label fx:id="status" text="None"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </StackPane>
                <LineChart GridPane.columnIndex="1" GridPane.rowIndex="0">
                    <xAxis>
                        <CategoryAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </LineChart>
                <LineChart GridPane.columnIndex="0" GridPane.rowIndex="1">
                    <xAxis>
                        <CategoryAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </LineChart>
                <LineChart GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <xAxis>
                        <CategoryAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </LineChart>
            </children>
        </GridPane>
    </children>
</AnchorPane>
